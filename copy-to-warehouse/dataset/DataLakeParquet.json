{
	"name": "DataLakeParquet",
	"properties": {
		"linkedServiceName": {
			"referenceName": "DataLake",
			"type": "LinkedServiceReference"
		},
		"parameters": {
			"FileSystem": {
				"type": "string",
				"defaultValue": "modern-data"
			},
			"Directory": {
				"type": "string",
				"defaultValue": "/1-raw/retail-adventureworks/"
			},
			"File": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "Parquet",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@dataset().File",
					"type": "Expression"
				},
				"folderPath": {
					"value": "@dataset().Directory",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().FileSystem",
					"type": "Expression"
				}
			},
			"compressionCodec": "snappy"
		},
		"schema": []
	}
